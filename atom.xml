---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>{{ site.title }} {{ site.description }}</title>
	<link href="https://www.jameskelso.com/atom.xml" rel="self"/>
	<link href="https://www.jameskelso.com"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>https://www.jameskelso.com</id>
	<author>
		<name>James Kelso</name>
		<email>james@jameskelso.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="https://www.jameskelso.com{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>https://www.jameskelso.com{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
